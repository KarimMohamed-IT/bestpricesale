@page "{slug}"
@model bestpricesale.Pages.PageShellModel
@{
    Layout = null;
    ViewData["Title"] = Model.Page?.Title;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@Model.Page?.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/site.css" />
</head>
<body>
    @Html.Raw(Model.Page?.Content)
</body>
</html>
